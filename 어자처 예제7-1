//클래스와 메소드 심층 탐구

class A { // 1.  클래스A
    private  int a;  //전용
    int b;          //디폴트
    public int c;       //공용
}


public class Main {  // 2. 메인 클래스
    public static void main(String[] args) {

        A obj = new A();


        // obj.a =10;  --   전용 멤버라 다른 클래스에서는 접근 안됨
        obj.b = 20;         //디폴트 접근 가능
        obj.c = 30;         //공용   접근 가능


    }
}
